    <!-- Making the jar file executable with its associated lib directory -->

    <properties>
        <plugins.param.jre.mainClass>webfx.platform.shared.services.appcontainer.ApplicationContainer</plugins.param.jre.mainClass>
        <!-- disabling classes-dependencies unpack to avoid including them again in the jar -->
        <plugin.dependency.param.unpack-classes.phase>none</plugin.dependency.param.unpack-classes.phase>
    </properties>

    <profiles>

        <!-- profile to make a non fat standalone executable jar with all dependent libraries unpacked in lib folder -->
        <profile>
            <id>mongoose-frontend-application-javafx-nonfat</id>
            <activation>
                <activeByDefault>false</activeByDefault>
            </activation>

            <build>
                <plugins>
                    <!-- Unpacking all dependent libraries in the lib folder -->
                    <plugin>
                        <artifactId>maven-dependency-plugin</artifactId>
                    </plugin>

                </plugins>
            </build>
        </profile>

        <!-- profile to make a fat standalone executable jar with all dependent libraries included inside -->
        <profile>
            <id>mongoose-frontend-application-javafx-fat</id>
            <activation>
                <activeByDefault>false</activeByDefault>
            </activation>

            <build>
                <plugins>

                    <!-- Calling Shade with the configuration defined in the parent pom -->
                    <plugin>
                        <groupId>org.apache.maven.plugins</groupId>
                        <artifactId>maven-shade-plugin</artifactId>
                    </plugin>

                </plugins>
            </build>
        </profile>

        <!-- Profile to minimize and obfuscate the jar file with Proguard -->
        <profile>
            <id>mongoose-frontend-application-javafx-min</id>
            <activation>
                <activeByDefault>false</activeByDefault>
            </activation>

            <build>
                <plugins>

                    <!-- Calling Proguard with the configuration defined in the parent pom -->
                    <plugin>
                        <groupId>com.github.wvengen</groupId>
                        <artifactId>proguard-maven-plugin</artifactId>
                    </plugin>

                </plugins>
            </build>
        </profile>

    </profiles>

